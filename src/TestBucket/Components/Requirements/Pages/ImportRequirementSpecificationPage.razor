@attribute [Authorize()]
@page "/{TenantId}/Requirements/Import"

@layout RequirementsLayout

<MudStack Class="container-settings-page">

    <MudText Class="mb-3" Typo="Typo.h1">Import Specification</MudText>

    <MudItem Class="pa-2">
        <UploadRequirementSpecification />
    </MudItem>

    <MudText>
        Import product documentation or requirement specifications
    </MudText>

</MudStack>

@code {
    [Parameter] public string TenantId { get; set; } = "";
    [CascadingParameter] TestProject? Project { get; set; }
    [CascadingParameter] Team? Team { get; set; }

}
