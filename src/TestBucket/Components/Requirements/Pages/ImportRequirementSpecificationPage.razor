@attribute [Authorize()]
@page "/{TenantId}/Requirements/Import"
@inject IAppLocalization loc

@layout RequirementsLayout

<MudStack Class="container-settings-page">

    <MudStack Spacing="0">
        <MudText Typo="Typo.h1">@loc.Requirements["import-requirements"]</MudText>
        <MudText Typo="Typo.subtitle2">@loc.Requirements["import-requirements-description"]</MudText>
    </MudStack>

    <MudItem Class="pa-2">
        <UploadRequirementSpecification />
    </MudItem>

</MudStack>

@code {
    [Parameter] public string TenantId { get; set; } = "";
    [CascadingParameter] TestProject? Project { get; set; }
    [CascadingParameter] Team? Team { get; set; }

}
